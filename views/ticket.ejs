  <section id="contact" class="parallax-section">
   <div class="container">
    <div class="row">

     <div class="col-md-12 col-sm-12">
      <!-- SECTION TITLE -->
      <div class="wow fadeInUp section-title" data-wow-delay="0.2s">
       <h2>¡Contáctanos!</h2>
       <p>Cualquier situación repórtala por este medio</p>
     </div>
   </div>

   <div class="col-md-7 col-sm-10">
    <!-- CONTACT FORM HERE -->
    <div class="wow fadeInUp" data-wow-delay="0.4s">
      <form id="contact-form" action="#" method="post">
        <div class="">
         <input type="text" id="nombre" class="form-control" name="name" value="<%= user.nombre%> <%= user.apellido%>" required="">
       </div>
       <br>
       <div class="">
         <input type="email" id="email" class="form-control" name="email" value="<%= user.username%>" required="">
       </div>
       <br>
       <div class="">
         <textarea class="form-control" id="mensaje" rows="5" name="message" placeholder="Mensaje o solicitud" required=""></textarea>
       </div>
       <br>
       <div class="col-md-offset-8 col-md-4 col-sm-offset-6 col-sm-6">
         <a id="submit" onclick="crearTicket(); return false;" class="btn btn-lg btn-primary btn-" >Enviar Información</a>
       </div>
     </form>
   </div>
 </div>


 <div class="col-md-5 col-sm-8">
  <!-- CONTACT INFO -->
  <div class="wow fadeInUp contact-info" data-wow-delay="0.4s">
   <div class="section-title">
    <br>
    <h2>Información de Contacto</h2>
    <p></p>
  </div>

  <img src="/images/logo.png" height="80px" width="120px">
  <p>C.P. 14010, Ciudad de México. </p>
  <p><i class="fa fa-comment"></i> <a href="mailto:support@.com">     support@.com</a></p>
  <p><i class="fa fa-phone"></i>    +52 (55) 56 29 88 66</p>
</div>
</div>

</div>
</div>
</section>



